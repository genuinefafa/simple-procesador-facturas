{
  "name": "procesador-facturas-server",
  "version": "0.2.0",
  "description": "Backend y servicios del procesador de facturas",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "rm -f ../data/database.sqlite && tsx scripts/migrate.ts"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^1.0.0-beta.11-05230d9",
    "exceljs": "^4.4.0",
    "heic-convert": "^2.1.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-to-img": "^5.0.0",
    "pdfjs-dist": "^3.11.174",
    "sharp": "^0.33.5",
    "tesseract.js": "^5.1.1",
    "yaml": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.1",
    "@types/pdf-parse": "^1.1.4",
    "@vitest/coverage-v8": "^4.0.10",
    "drizzle-kit": "^1.0.0-beta.11-05230d9",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=22.21.0",
    "npm": ">=10.0.0"
  }
}
